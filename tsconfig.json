{
    "compilerOptions": {
        "target": "ES6",
        "lib": ["ES6", "ES5"],
        "module": "CommonJS",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noEmitOnError": true,
        "noImplicitAny": true,
        "rootDir": "./src",
        "outDir": "./bin"
    }
}